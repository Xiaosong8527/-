   "source": [
    "# 类似于栈的先进后出模式\n",
    "# 1.要有递推关系\n",
    "# 2.要有临界值\n",
    "def digui(num):\n",
    "    print(num)\n",
    "    # 临界值\n",
    "    if num > 0:\n",
    "        # 这里调用的是本身的函数\n",
    "        digui(num-1)\n",
    "    else:\n",
    "        print('='*20)\n",
    "    print(num)\n",
    "digui(5)"
   ]
